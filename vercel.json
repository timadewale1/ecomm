{
  "rewrites": [
    {
      "source": "/product/:path*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(bot|crawler|spider|crawling)"
        }
      ],
      "destination": "/api/middleware"
    },
    {
      "source": "/store/:path*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(bot|crawler|spider|crawling)"
        }
      ],
      "destination": "/api/middleware"
    },
    {
      "source": "/marketstorepage/:path*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(bot|crawler|spider|crawling)"
        }
      ],
      "destination": "/api/middleware"
    }
  ]
}
