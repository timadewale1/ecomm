{
    "routes": [
      {
        "src": "/product/(.*)",
        "has": [
          { "type": "header", "key": "user-agent", "value": "(bot|crawler|spider|crawling)" }
        ],
        "dest": "/api/middleware"
      },
      {
        "src": "/store/(.*)",
        "has": [
          { "type": "header", "key": "user-agent", "value": "(bot|crawler|spider|crawling)" }
        ],
        "dest": "/api/middleware"
      },
      {
        "src": "/marketstorepage/(.*)",
        "has": [
          { "type": "header", "key": "user-agent", "value": "(bot|crawler|spider|crawling)" }
        ],
        "dest": "/api/middleware"
      }
    ],
    "rewrites": [
    {
      "source": "/static/*",
      "destination": "/static/*"
    },
    {
      "source": "/manifest.json",
      "destination": "/manifest.json"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
  }
  