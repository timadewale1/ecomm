{
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/product/(.*)",
        "has": [
          { "type": "header", "key": "user-agent", "value": "(bot|crawler|spider|crawling)" }
        ],
        "dest": "/api/prerender"
      },
      {
        "src": "/store/(.*)",
        "has": [
          { "type": "header", "key": "user-agent", "value": "(bot|crawler|spider|crawling)" }
        ],
        "dest": "/api/prerender"
      },
      {
        "src": "/marketstorepage/(.*)",
        "has": [
          { "type": "header", "key": "user-agent", "value": "(bot|crawler|spider|crawling)" }
        ],
        "dest": "/api/prerender"
      }
    ]
  }
  